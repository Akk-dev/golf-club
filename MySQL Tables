Table Memberships
create table Memberships(Member_Id varchar(6), Name varchar(30), Package varchar(7), 
duration varchar(10), Start_Date date, Upcoming_Event_Id varchar(5));
insert into Memberships values ("000001","Akshat Singhania","Gold", "1 year","2020-01-17", "A002");


#Table Events

create table Events(Event varchar(40), Date varchar(20),prize varchar(9), Exclusiv varchar(15),
status varchar(12));
insert into Events values("Avantha Masters Cup", "11th-17th March 2020","$1800000",
"Open for all", "Booked");
insert into Events values("Annual Championship", "28th-30th May 2020","$38000", "Members only", "Vacant");
