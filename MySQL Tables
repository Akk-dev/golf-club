# Table Courses

create table Courses(Tee varchar(9),Yardage integer(7),PAR integer(5),Course_Rating float(7), Slope_Rating integer(10));
insert into Courses values("Blue",7237,72,75.3,138);
insert into Courses values("White",6803,72,73.7,133);
insert into Courses values("Yellow",6065,72,69.4,124);
insert into Courses values("Red",5736,72,74.2,131);


# Table Names

create table Names(Name varchar(30),Phone varchar(11),Join_Date date,Member_ID varchar(6),sex varchar(2));


# Table Packages

create table Packages(Package_Name varchar (7), Duration varchar(10), price varchar(7));
insert into Packages values("Bronze", "6 months", "$650");
insert into Packages values("", "1 year", "$900");
insert into Packages values("", "2 year", "$1500");
insert into Packages values("", "", "");
insert into Packages values("Silver", "6 months", "$1000");
insert into Packages values("", "1 year", "$1700");
